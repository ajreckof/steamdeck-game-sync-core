    "version": "2.0.0",
    "tasks": [
        {
            "label": "clean project",
            "type": "shell",
            "command": "scons",
            "args": [
                "-c"
            ],
            "group": "build",
        },
        {
            "label": "generate project",
            "type": "shell",
            "command": "scons",
            "args": [
                "dev_build=true"         
            ],
            "group": "build",
        },
    ]
}